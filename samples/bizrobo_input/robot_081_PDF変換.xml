                <Switch/>
                <SendMail to="admin@example.com" subject="処理結果通知"/>
                <Assign variable="result" value="processed"/>
                <ExcelWriteRange sheet="結果" range="A1"/>
                <ExcelWriteRange sheet="結果" range="A1"/>
                <ExcelReadRange sheet="Sheet1" range="A1:Z100"/>
                <ExcelOpen filePath="C:\Data\data.xlsx"/>
