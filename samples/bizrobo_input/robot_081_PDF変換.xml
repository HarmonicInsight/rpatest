<?xml version="1.0" encoding="UTF-8"?>
<BizRoboRobot name="PDF変換_081" version="10.7">
    <Settings>
        <DesktopAutomation enabled="false"/>
        <Timeout value="96"/>
    </Settings>
    <Variables>
        <Variable name="inputPath" type="String" default="C:\Data\PDF変換_081.xlsx"/>
        <Variable name="outputPath" type="String" default="C:\Data\PDF変換_081.xlsx"/>
        <Variable name="dataTable" type="DataTable" default=""/>
