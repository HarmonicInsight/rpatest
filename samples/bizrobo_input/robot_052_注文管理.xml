<?xml version="1.0" encoding="UTF-8"?>
<BizRoboRobot name="注文管理_052" version="10.7">
    <Settings>
        <DesktopAutomation enabled="false"/>
        <Timeout value="58"/>
    </Settings>
    <Variables>
        <Variable name="filePath" type="String" default="C:\Data\注文管理_052.xlsx"/>
        <Variable name="dataTable" type="DataTable" default=""/>
        <Variable name="rowCount" type="Integer" default="0"/>
    </Variables>
    <Actions>
        <TryCatch>
        <ForEach collection="dataTable" itemVariable="row">
            <If condition="row != null">
                <Log message="処理完了" level="Info"/>
                <ExcelWriteCell sheet="Sheet1" cell="A42"/>
                <ExcelOpen filePath="C:\Data\input.xlsx"/>
                <ExcelReadRange sheet="Sheet1" range="A1:Z100"/>
            </If>
        </ForEach>
        </TryCatch>
    </Actions>
    <SubRobots>
        <SubRobot ref="共通_ログイン処理"/>
        <SubRobot ref="共通_メール送信"/>
    </SubRobots>
</BizRoboRobot>