<?xml version="1.0" encoding="UTF-8"?>
<BizRoboRobot name="旅費精算_038" version="10.7">
    <Settings>
        <DesktopAutomation enabled="false"/>
        <Timeout value="68"/>
    </Settings>
    <Variables>
        <Variable name="filePath" type="String" default="C:\Data\旅費精算_038.xlsx"/>
        <Variable name="dataTable" type="DataTable" default=""/>
        <Variable name="rowCount" type="Integer" default="0"/>
    </Variables>
    <Actions>
        <TryCatch>
        <ForEach collection="dataTable" itemVariable="row">
            <If condition="row != null">
                <ExcelOpen filePath="C:\Data\output.xlsx"/>
                <ExcelWriteCell sheet="Sheet1" cell="A16"/>
                <ExcelReadRange sheet="Sheet1" range="A1:Z100"/>
                <ExcelWriteCell sheet="Sheet1" cell="A38"/>
                <Log message="処理完了" level="Info"/>
                <ExcelReadRange sheet="Sheet1" range="A1:Z100"/>
                <Log message="処理完了" level="Info"/>
                <ExcelOpen filePath="C:\Data\output.xlsx"/>
            </If>
        </ForEach>
        </TryCatch>
    </Actions>
    <SubRobots>


    </SubRobots>
</BizRoboRobot>