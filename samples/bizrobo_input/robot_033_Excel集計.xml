<?xml version="1.0" encoding="UTF-8"?>
<BizRoboRobot name="Excel集計_033" version="10.7">
    <Settings>
        <DesktopAutomation enabled="false"/>
        <Timeout value="69"/>
    </Settings>
    <Variables>
        <Variable name="filePath" type="String" default="C:\Data\Excel集計_033.xlsx"/>
        <Variable name="dataTable" type="DataTable" default=""/>
        <Variable name="rowCount" type="Integer" default="0"/>
    </Variables>
    <Actions>
        <TryCatch>
        <ForEach collection="dataTable" itemVariable="row">
            <If condition="row != null">
                <ExcelReadRange sheet="Sheet1" range="A1:Z100"/>
                <ExcelOpen filePath="C:\Data\data.xlsx"/>
                <Log message="処理完了" level="Info"/>
                <Log message="処理完了" level="Info"/>
                <ExcelWriteCell sheet="Sheet1" cell="A90"/>
                <ExcelReadRange sheet="Sheet1" range="A1:Z100"/>
                <ExcelWriteCell sheet="Sheet1" cell="A20"/>
                <ExcelOpen filePath="C:\Data\output.xlsx"/>
            </If>
        </ForEach>
        </TryCatch>
    </Actions>
    <SubRobots>
        <SubRobot ref="共通_ログイン処理"/>

    </SubRobots>
</BizRoboRobot>